	RCO_PERIODO_ABIERTO bool NOT NULL,
	RCO_PERIODO_FINAL varchar(60) NOT NULL,
	RCO_PORCENTAJE_TEORICO int NULL,
	RCO_APLICA_SERVICIO_SOCIAL bool NOT NULL,
	RCO_SERVICO_SOCIAL_PORCENTAJE int NULL,
	RCO_SERVICIO_SOCIAL_HORAS int NULL,
	RCO_APLICA_TRANSFERENCIA_CONOCIMIENTO bool NOT NULL,
	RCO_TRANSFERENCIA_CONOCIMIENTO_PORCENTAJE int NULL,
	RCO_TRANSFERENCIA_CONOCIMIENTO_HORAS int NULL,
	RCO_APLICA_PERIODO_GRACIA bool NOT NULL,
	RCO_PERIODO_GRACIA_MESES int NULL,
	RCO_PERIODO_GRACIA_APLICACION varchar(60) NULL,
	RCO_APLICA_SUSPENCION_CONTINUA bool NOT NULL,
	RCO_SUSPENCION_CONTINUA_CANTIDAD int NULL,
	RCO_APLICA_SUSPENCION_DISCONTINUA bool NOT NULL,
	RCO_SUSPENCION_DISCONTINUA_CANTIDAD int NULL,
	RCO_APLICA_SUSPENCION_ESPECIAL bool NOT NULL,
	RCO_SUSPENCION_ESPECIAL_CANTIDAD int NULL,
	RCO_APLICA_PRORROGA bool NOT NULL,
	RCO_PRORROGA_CANTIDAD int NULL,
	RCO_APLICA_CODONACION_RENDIMEINTO_PERIODO bool NOT NULL,
	RCO_RENDIMIENTO_PERIODO LONGTEXT null,
	RCO_APLICA_CODONACION_RENDIMEINTO_ACUMULADO bool NOT NULL,
	RCO_RENDIMEINTO_ACUMULADO LONGTEXT null,
	RCO_USUARIO_MODIFICO varchar(15) null,
	RCO_FECHA_MODIFICO datetime null,
	RCO_USUARIO_CREO varchar(15) NOT NULL,
	RCO_FECHA_CREO varchar(100) NOT NULL,
	 PRIMARY KEY (RCO_CODIGO)
)
ENGINE=InnoDB
DEFAULT CHARSET=utf8
COLLATE=utf8_general_ci
COMMENT='RCO_REGLAMENTOS_CONDONACION';
