CREATE TABLE `aurora-fondos-dev`.CRT_CORTES (
	CRT_CODIGO int NOT NULL AUTO_INCREMENT,
	CRT_NOMBRE varchar(50) NOT NULL,
	CRT_DESDE date NOT NULL,
	CRT_HASTA date NOT NULL,
	CRT_USUARIO_MODIFICO varchar(15) NULL,
	CRT_FECHA_MODIFICO datetime NULL,
	CRT_USUARIO_CREO varchar(15) NULL,
	CRT_FECHA_CREO datetime NULL,
	 PRIMARY KEY (CRT_CODIGO)
)
ENGINE=InnoDB
DEFAULT CHARSET=utf8
COLLATE=utf8_general_ci
COMMENT='CRT_CORTES';
